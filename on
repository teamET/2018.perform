[32m+[m[32mrouter.post('/update', function(req, res, next) {[m[41m[m
[32m+[m[32m    //body„ÅÆÂèó„ÅëÂèñ„Çä[m[41m[m
[32m+[m[32m    var body = req.body;[m[41m[m
