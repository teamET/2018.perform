[32m+[m[41m[m
[32m+[m[32m/**[m[41m[m
[32m+[m[32m * LINE„ÅÆmessage„Ç™„Éñ„Ç∏„Çß„ÇØ„Éà„ÇíÁîüÊàê„Åô„Çã -> Build_responce„Å´Ê∏°„Åô[m[41m[m
[32m+[m[32m * @param  {} Token - replyToken[m[41m[m
[32m+[m[32m * @param  {} message1[m[41m [m
[32m+[m[32m * @param  {} [message2][m[41m[m
