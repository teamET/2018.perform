[32m+[m[32m            "tstamp":"tstamp"[m[41m[m
[32m+[m[32m        }];[m[41m[m
[32m+[m[32m        fs.writeFileSync('./public/events.json',JSON.stringify(events));[m[41m		[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
